{
	"name": "@objekt/checksum-generator",
	"version": "1.0.0",
	"description": "NodeJS utility library to generate checksums for files and directories.",
	"author": "Objekt Labs",
	"license": "MIT",
	"homepage": "https://github.com/objektlabs/checksum-generator",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/objektlabs/checksum-generator.git"
	},
	"bugs": {
		"url": "https://github.com/objektlabs/checksum-generator/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": "16.x"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 0",
		"lint": "eslint src/**/*.{js,ts}",
		"docs": "npm run docs:api && npm run docs:contributors",
		"docs:api": "npx typedoc --out docs src/index.ts",
		"docs:contributors": "npx all-contributors generate",
		"contributors:add": "npx all-contributors add",
		"prepublish": "npm run build"
	},
	"keywords": [
		"node",
		"checksum",
		"utility"
	],
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.36",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"all-contributors-cli": "^6.20.0",
		"eslint": "^8.15.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"typedoc": "^0.22.15",
		"typescript": "^4.6.4"
	}
}